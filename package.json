  "bugs": {
    "url": "https://github.com/Currzy/Currzy-js/issues"
  },
  "homepage": "https://github.com/Currzy/Currzy-js",
  "dependencies": {
    "fast-xml-parser": "^4.4.0",
    "ofetch": "^1.4.1"
  },
  "devDependencies": {
    "vitepress": "^2.0.0-alpha.12",
    "@types/node": "^22.7.6",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "eslint": "^8.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
